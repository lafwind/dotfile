[Appearance]
BoldIntense=false
ColorScheme=Lafwind_Night_V2
Font=Source Code Pro,10,-1,5,50,0,0,0,0,0

[General]
Command=/bin/zsh --login
Environment=TERM=xterm
LocalTabTitleFormat=%d:%n
Name=Shell
Parent=FALLBACK/
RemoteTabTitleFormat=%u%H
SilenceSeconds=1
StartInCurrentSessionDir=true

[Keyboard]
KeyBindings=default

[Scrolling]
HistoryMode=2
ScrollBarPosition=2

[Terminal Features]
BlinkingCursorEnabled=true
